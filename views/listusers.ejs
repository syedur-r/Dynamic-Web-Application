<!doctype html>         
	<head>                              
		<title>User List</title>                    
	</head>                 
	<h1 style="font-family:sans-serif;font-size:50px;"> This is the User List Page </h1>                 
	<body>                         
		<h3 style="font-family:sans-serif;font-size:25px;">You can see names, emails and usernames of all users here:</h3>            
		<ul>                                 
			<% users.forEach(function(user){ %>                                 
			<li style="font-family:sans-serif;font-size:20px;"><strong>Name:</strong> <%= user.firstname %> <%= user.lastname %><br/><strong>Email:</strong> <%= user.email%><br/><strong>Username:</strong> <%= user.username%><br/><br/></li>                                 
			<% }) %>                         
		</ul>                         
		<button type="button" onclick="darkMode()">Dark Mode</button>                                                               
		<button type="button" onclick="lightMode()">Light Mode</button>                                                               
		
		<script> function darkMode() {document.body.style.backgroundColor = "black";document.body.style.color = "white"; var elements = document.querySelectorAll("#nav"); for(var i = 0; i < elements.length; i++) elements[i].style.color = 'white';} function lightMode() {document.body.style.backgroundColor = "white";document.body.style.color = "black"; var elements =  document.querySelectorAll("#nav"); for(var i = 0; i  < elements.length; i++) elements[i].style.color = 'blue';} </script>

	</body>                 
	<br/><br/><a style="font-family:sans-serif;font-size:15px;" id="nav" href='./register'>Register</a> 
	<br/><br/><a style="font-family:sans-serif;font-size:15px;" id="nav" href='./deleteuser'>Delete User</a> 
	<br/><br/><a style="font-family:sans-serif;font-size:15px;" id="nav" href='./'>Home</a><br/><br/> 
</html>
